{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Asm": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
        }
      }
    ]
  },
  "ConnectionStrings": {
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Imaging": {
          "ImageFileTypes": [ "bmp", "jpeg", "jpg", "png", "webp", "gif", "avif", "tif", "tiff" ]
        },
        "Error404Collection": [
          {
            "Culture": "default",
            "ContentKey": "4df26cb8-3b72-40f9-94a7-28142d9cd214"
          }
        ]
      },
      "Examine": {
        "LuceneDirectoryFactory": "SyncedTempFileSystemDirectoryFactory"
      },
      "Global": {
        "Id": "0d74d531-73db-4ec5-a3c2-93c4bb639019",
        "TimeOut": "08:00:00",
        "MainDomLock": "SqlMainDomLock",
        "UseHttps": true
      },
      "Hosting": {
        "Debug": false,
        "LocalTempStorageLocation": "EnvironmentTemp"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "RequestHandler": {
        "EnableDefaultCharReplacements": false,
        "AddTrailingSlash": false
      },
      "Runtime": {
        "Mode": "Production"
      },
      "RuntimeMinification": {
        "CacheBuster": "Version"
      },
      "WebRouting": {
      }
    },
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ContainerName": "media"
        }
      }
    }
  },
  "AllowedHosts": "*"
}