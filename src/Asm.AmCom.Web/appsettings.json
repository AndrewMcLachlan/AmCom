{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": ""
        }
      }
    ]
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=.\\SQLExpress;Database=AmCom;Integrated Security=true"
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Imaging": {
          "ImageFileTypes": [ "bmp", "jpeg", "jpg", "png", "webp", "gif", "avif", "tif", "tiff" ]
        },
        "Error404Collection": [
          {
            "Culture": "default",
            "ContentKey": "4df26cb8-3b72-40f9-94a7-28142d9cd214"
          }
        ]
      },
      "Examine": {
        "LuceneDirectoryFactory": "SyncedTempFileSystemDirectoryFactory"
      },
      "Global": {
        "Id": "0d74d531-73db-4ec5-a3c2-93c4bb639019",
        "TimeOut": "08:00:00",
        "MainDomLock": "SqlMainDomLock"
      },
      "Hosting": {
        "Debug": false,
        "LocalTempStorageLocation": "EnvironmentTemp"
      },
      "RequestHandler": {
        "EnableDefaultCharReplacements": false,
        "AddTrailingSlash": false
      }
    },
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "",
          "ContainerName": "media"
        }
      }
    }
  },
  "AllowedHosts": "*"
}