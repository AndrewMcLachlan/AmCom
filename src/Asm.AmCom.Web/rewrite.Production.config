<?xml version="1.0" encoding="utf-8"?>
<rewrite xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <rules>
    <rule name="Canonical Host Name" enabled="false" xdt:Locator="Match(name)" xdt:Transform="SetAttributes(enabled)">
      <conditions>
        <add input="{HTTP_HOST}" pattern="^www\.andrewmclachlan\.com$" xdt:Locator="Match(input)" xdt:Transform="SetAttributes(pattern)" />
      </conditions>
      <action type="Redirect" url="https://www.andrewmclachlan.com/{R:1}" xdt:Locator="Match(type)" xdt:Transform="SetAttributes(url)" />
    </rule>
  </rules>
</rewrite>
