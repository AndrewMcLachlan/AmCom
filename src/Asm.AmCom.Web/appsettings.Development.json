{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=.\\SQLExpress;Database=AmCom;Integrated Security=true;TrustServerCertificate=true"
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Global": {
        "Smtp": {
          "From": "your@email.here",
          "Host": "localhost",
          "Port": 25
        },
        "UseHttps": false
      },
      "Hosting": {
        "Debug": true
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "RuntimeMinification": {
        "useInMemoryCache": true,
        "cacheBuster": "Timestamp"
      },
      "Runtime": {
        "Mode": "Development"
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://localhost:44369"
      }
    },
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": ""
        }
      }
    }
  }
}